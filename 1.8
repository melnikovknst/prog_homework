#include <stdio.h>
#include <math.h>


int main(void) {
    int floors, apartments, apartmentNumber;
    float outEntrance, entrance, outFloor;
    scanf("%d %d %d", &floors, &apartments, &apartmentNumber);
    
    entrance = floors * apartments;
    outEntrance = ceil((float)(apartmentNumber / entrance));
    outFloor = ceil((apartmentNumber - ((outEntrance-1) * apartments * floors)) / apartments);
    printf("%d, %d\n", (int)outEntrance, (int)outFloor);

    return 0;
}

