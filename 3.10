#include <stdio.h>
#include <math.h>


int factorial(int digit) {
    if (digit == 0)
        return 1;
    else
        return digit * factorial(digit - 1);
}


int main(void) {
    int i, j, n;
    float sum = 0.0;
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        float inter_sum = 1.0;
        for (j = i; j < 2 * n; j++) {
            inter_sum *= (pow(-1, i) * (j + 1)) / (factorial(j - i + 1));
        }
        sum += inter_sum;
    }
    printf("%f", sum);

    return 0;
}
