#include <stdio.h>


int main(void) {
    int lB1x, lB1y, rT1x, rT1y, lB2x, lB2y, rT2x, rT2y;    // l - left, r - right, B - bottom, T - top
    scanf("%d %d %d %d %d %d %d %d", &lB1x, &lB1y, &rT1x, &rT1y, &lB2x, &lB2y, &rT2x, &rT2y);

    if (lB1x < lB2x) printf("(%d,", lB1x);
    else printf("(%d,", lB2x);
    
    if (lB1y < lB2y) printf("%d) ", lB1y);
    else printf("%d) ", lB2y);
    
    if (rT2x < rT1x) printf("(%d,", rT1x);
    else printf("(%d,", rT2x);
    
    if (rT2y < rT1y) printf("%d)\n", rT1y);
    else printf("%d)\n", rT2y);

    return 0;
}
