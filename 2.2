#include <stdio.h>


int main(void) {
    int left_bottom_1_x, left_bottom_1_y, right_top_1_x, right_top_1_y, left_bottom_2_x, left_bottom_2_y, right_top_2_x, right_top_2_y;
    scanf("%d %d %d %d %d %d %d %d", &left_bottom_1_x, &left_bottom_1_y, &right_top_1_x, &right_top_1_y, &left_bottom_2_x, &left_bottom_2_y, &right_top_2_x, &right_top_2_y);

    if (left_bottom_1_x < left_bottom_2_x)
        printf("(%d,", left_bottom_1_x);
    else
        printf("(%d,", left_bottom_2_x);

    if (left_bottom_1_y < left_bottom_2_y)
        printf("%d) ", left_bottom_1_y);
    else
        printf("%d) ", left_bottom_2_y);
    if (right_top_2_x < right_top_1_x)
        printf("(%d,", right_top_1_x);
    else
        printf("(%d,", right_top_2_x);
    if (right_top_2_y < right_top_1_y)
        printf("%d)\n", right_top_1_y);
    else
        printf("%d)\n", right_top_2_y);

    return 0;
}
