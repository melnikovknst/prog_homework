#include <stdio.h>
#include <math.h>

int main(void){
    float y, hours_angle;
    scanf("%f", &y);
    hours_angle = (y * 180 / M_PI);

    printf("%f ", (hours_angle - 30 * (floor)(hours_angle / 30)) * 12);  //Minutes angle
    printf("%d ", (int)(hours_angle / 30));  //Hours
    printf("%d\n", (int)((hours_angle - 30 * (floor)(hours_angle / 30)) * 12)/6); //Minutes

    return 0;
}
