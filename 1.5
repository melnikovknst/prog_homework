#include <stdio.h>
#include <math.h>

int main(void){
    float y, hours_angle;
    scanf("%f", &y);
    hours_angle = (y * 180 / M_PI);

    printf("Minutes angle: %f\n", (hours_angle - 30 * (floor)(hours_angle / 30)) * 12);
    printf("Hours passed: %d\n", (int)(hours_angle / 30));
    printf("Minutes passed: %d\n", (int)((hours_angle - 30 * (floor)(hours_angle / 30)) * 12)/6);

    return 0;
}
